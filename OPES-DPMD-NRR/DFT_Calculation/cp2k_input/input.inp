&GLOBAL
  PROJECT DPGEN
  RUN_TYPE ENERGY_FORCE
&END GLOBAL
&FORCE_EVAL
  METHOD Quickstep
  STRESS_TENSOR ANALYTICAL
  &DFT #FORCE_EVAL
    BASIS_SET_FILE_NAME BASIS_MOLOPT
    POTENTIAL_FILE_NAME GTH_POTENTIALS
    CHARGE 0
    UKS  
    MULTIPLICITY 2
    &MGRID #DFT #FORCE_EVAL
      CUTOFF 800
      REL_CUTOFF 40
      NGRIDS 20
    &END MGRID #DFT #FORCE_EVAL
    &QS #DFT #FORCE_EVAL
      EPS_DEFAULT 1.0E-11
    &END QS #DFT #FORCE_EVAL
    &SCF #DFT #FORCE_EVAL
      SCF_GUESS ATOMIC
      EPS_SCF 3.0E-7
      MAX_SCF 128
      &DIAGONALIZATION #SCF #DFT #FORCE_EVAL
        ALGORITHM STANDARD
      &END DIAGONALIZATION #SCF #DFT #FORCE_EVAL
      &MIXING #SCF #DFT #FORCE_EVAL
        METHOD BROYDEN_MIXING
        ALPHA 0.4
        NBROYDEN 8
      &END MIXING #SCF #DFT #FORCE_EVAL
      &SMEAR #SCF #DFT #FORCE_EVAL
        METHOD FERMI_DIRAC
        ELECTRONIC_TEMPERATURE 300
      &END SMEAR #SCF #DFT #FORCE_EVAL
      ADDED_MOS 150
    &END SCF #DFT #FORCE_EVAL
    &XC #DFT #FORCE_EVAL
      &XC_FUNCTIONAL PBE
      &END XC_FUNCTIONAL #XC #DFT #FORCE_EVAL
      &VDW_POTENTIAL #XC #DFT #FORCE_EVAL
        POTENTIAL_TYPE PAIR_POTENTIAL
        &PAIR_POTENTIAL #VDW_POTENTIAL #XC #DFT #FORCE_EVAL
          PARAMETER_FILE_NAME dftd3.dat
          TYPE DFTD3
          REFERENCE_FUNCTIONAL PBE
        &END PAIR_POTENTIAL #VDW_POTENTIAL #XC #DFT #FORCE_EVAL
      &END VDW_POTENTIAL #XC #DFT #FORCE_EVAL
    &END XC #DFT #FORCE_EVAL
  &END DFT #FORCE_EVAL
  &SUBSYS #FORCE_EVAL
    &CELL #SUBSYS #FORCE_EVAL
      A 12.3302207  0.         0.       
      B  0.         12.81394162  0.        
      C  0.  0. 16.
    &END CELL #SUBSYS #FORCE_EVAL
    &COORD #SUBSYS #FORCE_EVAL
      @include coord.xyz
    &END COORD #SUBSYS #FORCE_EVAL
    &KIND Ru
      MAGNETIZATION 3
      BASIS_SET DZVP-MOLOPT-SR-GTH-q16
      POTENTIAL GTH-PBE-q16
    &END KIND #SUBSYS #FORCE_EVAL
    &KIND C
      MAGNETIZATION 0.6
      BASIS_SET DZVP-MOLOPT-SR-GTH-q4
      POTENTIAL GTH-PBE-q4
    &END KIND #SUBSYS #FORCE_EVAL
    &KIND N
      MAGNETIZATION 1
      BASIS_SET DZVP-MOLOPT-SR-GTH-q5
      POTENTIAL GTH-PBE-q5
    &END KIND #SUBSYS #FORCE_EVAL
    &KIND O
      MAGNETIZATION 0.6
      BASIS_SET DZVP-MOLOPT-SR-GTH-q6
      POTENTIAL GTH-PBE-q6
    &END KIND #SUBSYS #FORCE_EVAL
    &KIND H
      MAGNETIZATION 0.6
      BASIS_SET DZVP-MOLOPT-SR-GTH-q1
      POTENTIAL GTH-PBE-q1
    &END KIND #SUBSYS #FORCE_EVAL
  &END SUBSYS #FORCE_EVAL
  &PRINT #FORCE_EVAL
    &FORCES ON
    &END FORCES #PRINT #FORCE_EVAL
    &STRESS_TENSOR ON
    &END STRESS_TENSOR #PRINT #FORCE_EVAL
  &END PRINT #FORCE_EVAL
&END FORCE_EVAL